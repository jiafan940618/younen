	<select id="findAllAm3Phase" resultMap="Am3PhaseMapper"
		parameterType="java.lang.String">
		select * from am_3phase_${sqlNowData} where
		meter_time <![CDATA[ >= ]]>
		${startDtm}
		and meter_time <![CDATA[ <= ]]>
		${endDtm}
	</select>


